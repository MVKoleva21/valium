{
  "routes": [
      {
          "route": "/accounts/signin",
          "allowedRoles": [
              "anonymous",
              "authenticated"
          ]
      },
      {
          "route": "/accounts/signup",
          "allowedRoles": [
              "anonymous",
              "authenticated"
          ]
      },
      {
          "route": "/accounts/finilize",
          "allowedRoles": [
              "authenticated"
          ]
      },
      {
          "route": "/wills",
          "allowedRoles": [
              "authenticated"
          ]
      },
      {
          "route": "/wallet",
          "allowedRoles": [
              "authenticated"
          ]
      },
      {
          "route": "/will",
          "allowedRoles": [
              "authenticated"
          ]
      },
      {
          "route": "/account",
          "allowedRoles": [
              "authenticated"
          ]
      },
      
  ],
  "navigationFallback": {
      "rewrite": "index.html",
      "exclude": [
          "/images/.{png,jpg,gif}",
          "/css/*"
      ]
  },
  "responseOverrides": {
      "400": {
          "rewrite": "/"
      },
      "401": {
          "redirect": "/"
      },
      "403": {
          "rewrite": "/"
      },
      "404": {
          "rewrite": "/index.html",
          "statusCode": 200
      }
  },
  "mimeTypes": {
      ".html": "text/html",
      ".json": "text/json",
      ".css": "text/css",
      ".js": "text/javascript"
  }

}
